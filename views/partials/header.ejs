    <meta name="og:image" content="images/logo.png" />
    <!-- For IE -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- For Resposive Device -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- For Window Tab Color -->
    <!-- Chrome, Firefox OS and Opera -->
    <meta name="theme-color" content="#1d2b40" />
    <!-- Windows Phone -->
    <meta name="msapplication-navbutton-color" content="#1d2b40" />
    <!-- iOS Safari -->
    <meta name="apple-mobile-web-app-status-bar-style" content="#1d2b40" />
    <title><%= locals.title %> | Cihan Media Group</title>
    <!-- Favicon -->
    <link rel="icon" type="image/png" sizes="56x56" href="/images/logo2.png" />
    <!-- Main style sheet -->
    <link rel="stylesheet" type="text/css" href="/css/style.css" media="all" />
    <!-- responsive style sheet -->
    <link
